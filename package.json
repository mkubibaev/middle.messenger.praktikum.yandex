    "serve": "parcel src/*.html --no-source-maps && parcel watch ./*",
    "start": "node server.js",
    "lint": "eslint . --ext .ts",
    "stylelint": "stylelint '**/*.pcss'"
  },
  "alias": {
    "core": "./src/core",
    "types": "./src/types",
    "helpers": "./src/helpers",
    "pages": "./src/pages",
    "components": "./src/components",
    "utils": "./src/utils",
    "api": "./src/api",
    "services": "./src/services"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkubibaev/middle.messenger.praktikum.yandex.git"
